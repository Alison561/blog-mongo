<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.tiny.cloud/1/74czl6vhyu8fuum9mmvwtbjyu6rnmbx75blg4ywfqdx7ylah/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>
    <title>Blog</title>
</head>
<body>
    <form method="post" enctype="multipart/form-data">
        <input type="file" name="file">
        <input type="text" name="assunt">
        <input type="text" name="desc">
        <select name="categoria" multiple size="3">
            <option value="">Selecione uma cartegoria</option>
            <% categoria.forEach(e => { %>
                <option value="<%= e.nome %> "><%= e.nome %></option>
            <% }) %>
        </select>
        <textarea name="texto"></textarea>
        <button>Cadastrar</button>
    </form>
</body>
<script>
    tinymce.init({
      selector: 'textarea',
      plugins: 'advlist autolink lists link image charmap print preview hr anchor pagebreak',
      toolbar_mode: 'floating',
   });
  </script>
</html>